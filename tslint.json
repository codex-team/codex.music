{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-standard",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": false,
        "no-console": false,
        "await-promise": true,
        "deprecation": true,
        "no-floating-promises": true,
        "no-unnecessary-qualifier": true,
        "no-unnecessary-type-assertion": true,
        "strict-type-predicates": true
    },
    "rulesDirectory": []
}
